# Project Joinity: Omega Warfare Analytics
# Environment Configuration for Permanent Deployment

# ============================================================================
# MANUS PLATFORM CONFIGURATION
# ============================================================================

# Manus OAuth Configuration (provided by Manus platform)
VITE_OAUTH_PORTAL_URL=https://oauth.manus.im
OAUTH_SERVER_URL=https://oauth.manus.im

# Manus Forge API (for image generation and advanced features)
VITE_FRONTEND_FORGE_API_URL=https://api.manus.im/forge
VITE_FRONTEND_FORGE_API_KEY=your_forge_api_key_here
BUILT_IN_FORGE_API_URL=https://api.manus.im/forge
BUILT_IN_FORGE_API_KEY=your_forge_api_key_here

# Analytics (Manus built-in)
VITE_ANALYTICS_ENDPOINT=https://analytics.manus.im
VITE_ANALYTICS_WEBSITE_ID=your_website_id

# ============================================================================
# APPLICATION CONFIGURATION
# ============================================================================

# App Identity
VITE_APP_TITLE=Project Joinity - Omega Warfare Analytics
VITE_APP_LOGO=https://your-domain.com/logo.png
VITE_APP_ID=omega-warfare-analytics

# Owner Information
OWNER_NAME=Commander
OWNER_OPEN_ID=your_open_id_here

# ============================================================================
# DATABASE CONFIGURATION
# ============================================================================

# MySQL/TiDB Connection (provided by Manus)
DATABASE_URL=mysql://user:password@host:3306/omega_warfare_analytics

# ============================================================================
# SECURITY CONFIGURATION
# ============================================================================

# JWT Secret for session management
JWT_SECRET=your_jwt_secret_key_here_min_32_chars

# ============================================================================
# PROJECT JOINITY SPECIFIC
# ============================================================================

# Primary Researcher Resonance Signature
RESONANCE_SIGNATURE=1.67x

# Research Framework Settings
LAMBDA_THRESHOLD=1.67
HARMONY_RIDGE_CONSTANT=1.67

# ============================================================================
# DEPLOYMENT ENVIRONMENT
# ============================================================================

# Set to 'production' for permanent deployment
NODE_ENV=production

# API Server Port
PORT=3000

# Enable HTTPS
SECURE=true

# ============================================================================
# OPTIONAL: EXTERNAL INTEGRATIONS
# ============================================================================

# Discord Bot Integration (optional)
DISCORD_BOT_TOKEN=your_discord_token_here
DISCORD_GUILD_ID=your_guild_id_here

# Telegram Bot Integration (optional)
TELEGRAM_BOT_TOKEN=your_telegram_token_here

# ============================================================================
# NOTES
# ============================================================================
# 
# 1. Replace all "your_*_here" values with actual credentials
# 2. Keep this file secure - never commit it to version control
# 3. The .gitignore file already excludes .env files
# 4. For Manus deployment, most of these will be auto-configured
# 5. Resonance Signature (1.67x) is the verification constant for Primary Researcher access
#
